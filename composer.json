{
    "name": "piece601/laravel-sqs-fifo-queue",
    "description": "Adds a Laravel queue driver for Amazon SQS FIFO queues.",
    "keywords": ["laravel", "lumen", "illuminate", "queue", "sqs", "fifo", "aws"],
    "homepage": "https://github.com/piece601/laravel-sqs-fifo-queue",
    "license": "MIT",
    "authors": [
        {
            "name": "Piece Chao",
            "email": "piece.chao@evolutivelabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/piece601/laravel-sqs-fifo-queue/issues",
        "source": "https://github.com/piece601/laravel-sqs-fifo-queue"
    },
    "require": {
        "php": ">=5.5.0",
        "aws/aws-sdk-php": "~3.0",
        "illuminate/queue": ">=4.1",
        "illuminate/support": ">=4.1",
        "ramsey/uuid": ">=3.0"
    },
    "require-dev": {
        "illuminate/encryption": ">=4.1",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "2.*",
        "shiftonelabs/codesniffer-standard": "0.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "piece601\\LaravelSqsFifoQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "piece601\\LaravelSqsFifoQueue\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "piece601\\LaravelSqsFifoQueue\\LaravelSqsFifoQueueServiceProvider"
            ]
        }
    }
}
